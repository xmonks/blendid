{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "forkProcessing": "enabled",
  "extends": [
    "config:base",
    "schedule:daily",
    "group:postcss",
    ":preserveSemverRanges",
    ":dependencyDashboard"
  ],
  "automerge": true,
  "automergeStrategy": "rebase",
  "packageRules": [
    {
      "groupName": "marked",
      "matchPackageNames": ["marked"],
      "matchPackagePrefixes": ["marked-"]
    }
  ]
}
